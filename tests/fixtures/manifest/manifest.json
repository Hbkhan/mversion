{
  "name": "Example Manifest file",
  "description": "This is a fictive manifest file used by mversion tests",
  "homepage_url": "https://github.com/mikaelbr/mversion",
  "version": "0.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Some Title",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "premission1",
    "premission3",
    "premission2",
    "*://*/*"
  ]
}
