#!/usr/bin/env node
var argv = process.argv.slice(2);

var logger = console.log.bind(console);
var errorLogger = console.error.bind(console);

require('./cli')(argv, {
  logger: logger,
  errorLogger: errorLogger
}, function (err) {
  setTimeout(function () {
    if (err) process.exit(1);
    process.exit(0);
  }, 0);
});
